{
    "name": "emnify/emnify-sdk",
    "description": "Supply your swarm of IoT devices with cloud connectivity by emnify. Automate your routines with this SDK for PHP.    ",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://github.com/CodericEcosystem/emnify-php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Neftali Yagua",
            "email": "despacho@neftaliyagua.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.7",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5.13",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Emnify\\EmnifySdk\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Emnify\\EmnifySdk\\": "test/"
        }
    },
    "type": "library",
    "scripts": {
        "test": "./vendor/bin/phpunit test"
    }
}